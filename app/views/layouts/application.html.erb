<!DOCTYPE html>
<html>
<head>
  <title>TodoApp</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if flash["notice"] %>
<% flash["notice"].each do |notice| %>
<%= notice %>
<% end %>
<% end %>
<%= yield %>

</body>
</html>
