<%= form_for @project do |f| %>
<!-- for organization, maybe put some blank lines between
field sets -->
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.collection_select :team_id, Team.all, :id, :name  %>
  <%= f.submit %>
<% end %>
